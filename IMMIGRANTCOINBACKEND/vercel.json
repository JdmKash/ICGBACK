{
  "version": 2,
  "builds": [{
    "src": "api/webhook.py",
    "use": "@vercel/python"
  }],
  "routes": [
    {
      "src": "/api/webhook",
      "methods": ["POST"],
      "dest": "api/webhook.py"
    },
    {
      "src": "/",
      "methods": ["GET"],
      "dest": "api/webhook.py"
    }
  ]
}